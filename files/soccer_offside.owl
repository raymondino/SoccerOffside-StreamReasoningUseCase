    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/hasPosition -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;hasPosition">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:range rdf:resource="&Soccer_Offside;Position"/>
        <owl:inverseOf rdf:resource="&Soccer_Offside;isPositionOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&Soccer_Offside;Ball"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Glove"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Player"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Referee"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/hasTeam -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;hasTeam">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&Soccer_Offside;Player"/>
        <rdfs:range rdf:resource="&Soccer_Offside;Team"/>
        <owl:inverseOf rdf:resource="&Soccer_Offside;isTeamOf"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/hasTeammate -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;hasTeammate">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
